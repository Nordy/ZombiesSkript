command /getgun [<text>] [<number>]:
    trigger:
        if "%{zombies::itemlist::*}%" contains arg-1:
            if arg-2 is set:
                set slot 1 of player's inventory to {zombies::items::%arg-1%::clip} of {zombies::items::%arg-1%::item}
                set {zombies::gun::%arg-1%::ammo::%player%} to arg-2
                set {zombies::gun::%arg-1%::reload::%player%} to false
                set {zombies::gun::%arg-1%::firerate::%player%} to false 
                send "&a(!) You have been given a %arg-1% with %arg-2% ammo!"
            else:
                send "&c(!) Please specify the amount of ammo you want to have!"
        else:
            send "&c(!) Gun not found!"
            send "&c(!) Available guns: %{zombies::itemlist::*}%"

on skbee tab complete for "/getgun":
    loop {zombies::itemlist::*}:
        add loop-value to tab completions


        
