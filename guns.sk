function creategun(name: text, item: item, displayname: text, lore: texts, damage: number, ammo: number, clip: number, firerate: number, reload: number, particle: text, goldshot: number, goldcrit: number, sound: text, pitch: number, reference: text = "none", shotgun: number = 0, burst: number = 1, burstcooldown: number = 0):
    if {_reference} is "none":
        if {_shotgun} is bigger than 0:
            broadcast "%{_name}%, %{_reference}%"
            set {zombies::items::%{_name}%::item} to {_item} named {_displayname} with lore {_lore::*}, " ", "&7❤ Damage: &a%{_damage}%*%{_shotgun}% HP", "&7✏ Ammo: &a%{_ammo}%", "&7✒ Clip ammo: &a%{_clip}%", "&7⌛ Fire rate: &a%{_firerate}%s", "&7↩  Reload: &a%{_reload}%s", "&7＄ Gold: &a%{_goldshot}% (%{_goldcrit}%)", " ", "&bLEFT CLICK &7to reload." and "&bRIGHT CLICK &7to shoot." with attributes hidden 
        else:
            set {zombies::items::%{_name}%::item} to {_item} named {_displayname} with lore {_lore::*}, " ", "&7❤ Damage: &a%{_damage}% HP", "&7✏ Ammo: &a%{_ammo}%", "&7✒ Clip ammo: &a%{_clip}%", "&7⌛ Fire rate: &a%{_firerate}%s", "&7↩  Reload: &a%{_reload}%s", "&7＄ Gold: &a%{_goldshot}% (%{_goldcrit}%)", " ", "&bLEFT CLICK &7to reload." and "&bRIGHT CLICK &7to shoot." with attributes hidden
    else:
        if {_shotgun} is bigger than 0:
            set {zombies::items::%{_name}%::item} to glowing {_item} named {_displayname} with lore {_lore::*}, " ", "&7❤ Damage: &a&m%{zombies::items::%{_reference}%::damage}%*%{zombies::items::%{_reference}%::shotgun}% HP&6  --> %{_damage}%*%{_shotgun}% HP", "&7✏ Ammo: &a&m%{zombies::items::%{_reference}%::ammo}%&6  -->  %{_ammo}%", "&7✒ Clip ammo: &a&m%{zombies::items::%{_reference}%::clip}%&6  -->  %{_clip}%", "&7⌛ Fire rate: &a&m%{zombies::items::%{_reference}%::firerate}%s&6  -->  %{_firerate}%s", "&7↩  Reload: &a&m%{zombies::items::%{_reference}%::reload}%s&6  -->  %{_reload}%s", "&7＄ Gold: &a&m%{zombies::items::%{_reference}%::goldshot}% (%{zombies::items::%{_reference}%::goldcrit}%)&6  -->  %{_goldshot}% (%{_goldcrit}%)", " ", "&bLEFT CLICK &7to reload." and "&bRIGHT CLICK &7to shoot." with attributes hidden
        else:
            set {zombies::items::%{_name}%::item} to glowing {_item} named {_displayname} with lore {_lore::*}, " ", "&7❤ Damage: &a&m%{zombies::items::%{_reference}%::damage}% HP&6  --> %{_damage}% HP", "&7✏ Ammo: &a&m%{zombies::items::%{_reference}%::ammo}%&6  -->  %{_ammo}%", "&7✒ Clip ammo: &a&m%{zombies::items::%{_reference}%::clip}%&6  -->  %{_clip}%", "&7⌛ Fire rate: &a&m%{zombies::items::%{_reference}%::firerate}%s&6  -->  %{_firerate}%s", "&7↩  Reload: &a&m%{zombies::items::%{_reference}%::reload}%s&6  -->  %{_reload}%s", "&7＄ Gold: &a&m%{zombies::items::%{_reference}%::goldshot}% (%{zombies::items::%{_reference}%::goldcrit}%)&6  -->  %{_goldshot}% (%{_goldcrit}%)", " ", "&bLEFT CLICK &7to reload." and "&bRIGHT CLICK &7to shoot." with attributes hidden

    set {zombies::items::%{_name}%::damage} to {_damage}
    set {zombies::items::%{_name}%::ammo} to {_ammo}
    set {zombies::items::%{_name}%::clip} to {_clip}
    set {zombies::items::%{_name}%::firerate} to {_firerate}
    set {zombies::items::%{_name}%::reload} to {_reload}
    set {zombies::items::%{_name}%::particle} to {_particle}
    set {zombies::items::%{_name}%::goldshot} to {_goldshot}
    set {zombies::items::%{_name}%::goldcrit} to {_goldcrit}
    set {zombies::items::%{_name}%::sound} to {_sound}
    set {zombies::items::%{_name}%::pitch} to {_pitch}
    set {zombies::items::%{_name}%::shotgun} to {_shotgun}
    set {zombies::items::%{_name}%::burst} to {_burst}
    set {zombies::items::%{_name}%::burstcooldown} to {_burstcooldown}
    send "&a(!) Registered custom gun %{_name}%!" to console
    add {_name} to {zombies::itemlist::*}

