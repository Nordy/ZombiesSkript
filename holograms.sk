function buyitemholo(id: text, display: text, item: item, cost: number, location: location):
    delete the hologram with the id "buyitem-%{_id}%"
    set {zombies::items::costs::guns::%{_id}%::cost} to {_cost}
    create a hd hologram at {_location} with the id "buyitem-%{_id}%"
    add the text line "&6&l&n%{_display}%" to the hologram with id "buyitem-%{_id}%"
    add the text line "&7%{_cost}% Gold" to the hologram with id "buyitem-%{_id}%"
    add the item {_item} to the hologram with id "buyitem-%{_id}%"
    add the text line "" to the hologram with id "buyitem-%{_id}%"
    loop 4 times:
        make line loop-value of the hologram with id "buyitem-%{_id}%" clickable
    send "&a[Zombies (&lHolos&a)] Registered custom hologram buyitem-%{_id}%!" to console


function closedwinholo(id: text, level: number, location: location):
    delete the hologram with the id "closedwin-%{_id}%"
    create a hd hologram at {_location} with the id "closedwin-%{_id}%"
    add the text line "&c&l&o&nCLOSED WINDOW" to the hologram with id "closedwin-%{_id}%"
    add the text line "&7&l&oLEVEL %{_level}%" to the hologram with id "closedwin-%{_id}%"
    send "&a[Zombies (&lHolos&a)] Registered custom hologram closedwin-%{_id}%!" to console

