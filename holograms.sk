on hd hologram click:
    broadcast "%id of event-hologram%"

on click on armor stand:
    broadcast "%name of event-entity%"

command /holocoords:
    trigger:
        if {holocoord::%player%} is not true:
            set {holocoord::%player%} to true
            send "&aYep"
        else:
            set {holocoord::%player%} to false
            send "&cNope"

on break:
    if {holocoord::%player%} is true:
        cancel event
        send "&b%event-block%"

