function buyarmorholo(id: text, display: text, cost: number, type: string, material: string, location: location, invisible: boolean):
    set {zombies::items::costs::armors::%{_id}%::cost} to {_cost}
    delete the hologram with the id "buyarmor-%{_id}%"
    if {_invisible} is true:
        create a hd hologram at {_location} with the id "buyarmor-%{_id}%" to be invisible by default
    else:
        create a hd hologram at {_location} with the id "buyarmor-%{_id}%"
    add the text line "&6&l&n%{_display}%" to the hologram with id "buyarmor-%{_id}%"
    if {_cost} is not -1:
        add the text line "&7%{_cost}% Gold" to the hologram with id "buyarmor-%{_id}%"
    else:
        add the text line "&a&l&oYOU HAVE THIS ALREADY!" to the hologram with id "buyarmor-%{_id}%"
    
    if {_type} is "upper":
        add the item ("%{_material}% helmet" parsed as itemtype) to the hologram with id "buyarmor-%{_id}%"
        add the item ("%{_material}% chestplate" parsed as itemtype) to the hologram with id "buyarmor-%{_id}%"

    else if {_type} is "lower":
        add the item ("%{_material}% leggings" parsed as itemtype) to the hologram with id "buyarmor-%{_id}%"
        add the item ("%{_material}% boots" parsed as itemtype) to the hologram with id "buyarmor-%{_id}%"

    add the text line "" to the hologram with id "buyarmor-%{_id}%"
    loop 5 times:
        make line loop-value of the hologram with id "buyarmor-%{_id}%" clickable

function buyperkholo(id: text, display: text, item: item, cost: number, location: location, rotation: number):
    delete the hologram with the id "buyperk-%{_id}%"
    kill {zombies::armorstands::perks::%{_id}%}
    set {zombies::items::costs::perks::%{_id}%::cost} to {_cost}
    if {_rotation} is 0:
        set {_location2} to location(x-coordinate of {_location}, y-coordinate of {_location}-3, z-coordinate of {_location}+0.3)
    else if {_rotation} is 90:
        set {_location2} to location(x-coordinate of {_location}+0.3, y-coordinate of {_location}-3, z-coordinate of {_location})
    else if {_rotation} is 180:
        set {_location} to location(x-coordinate of {_location}, y-coordinate of {_location}-3, z-coordinate of {_location}-0.3)
    else if {_rotation} is -90:
        set {_location} to location(x-coordinate of {_location}-0.3, y-coordinate of {_location}-3, z-coordinate of {_location})
    else:
        set {_location2} to {_location}
    spawn armor stand at {_location2} with nbt from "{NoGravity:1b,Invisible:1b,Invulnerable:1b,DisabledSlots:2039583,Rotation:[%{_rotation}%f,0f]}"
    set {zombies::armorstands::perks::%{_id}%} to last spawned armor stand
    set name of {zombies::armorstands::perks::%{_id}%} to "buyperk-%{_id}%"
    set {zombies::armorstands::perks::%{_id}%}'s helmet to {_item}
    create a hd hologram at {_location} with the id "buyperk-%{_id}%"
    add the text line "&6&l&n%{_display}%" to the hologram with id "buyperk-%{_id}%"
    add the text line "&7%{_cost}% Gold" to the hologram with id "buyperk-%{_id}%"
    add the text line "" to the hologram with id "buyperk-%{_id}%"
    add the text line "" to the hologram with id "buyperk-%{_id}%"
    add the text line "" to the hologram with id "buyperk-%{_id}%"
    loop 5 times:
        make line loop-value of the hologram with id "buyperk-%{_id}%" clickable


